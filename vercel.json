{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "PORT": "5000",
    "JWT_SECRET": "your_jwt_secret_here",
    "MONGODB_URI": "your_mongodb_connection_string_here"
  }
}
